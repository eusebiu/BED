	pmcid = {PMC3170168},
	keywords = {Animals, Classification, Computational Biology, Databases, Genetic, Genome, Humans, Information Storage and Retrieval, Polymorphism, Single Nucleotide, Search Engine, Anopheles, Open Reading Frames},
